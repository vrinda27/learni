{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
        "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
        "screens/*":["./src/screens/*"],
        "component/*":["./src/component/*"],
        "global/*":["./src/global/*"],
        "assets/*":["./src/assets/*"],
        "navigation/*":["./src/navigation/*"],
        "modals/*":["./src/modals/*"],
        "reduxTooklit/*":["./src/reduxTooklit/*"]
  }
},
"exclude": [
  "node_modules",
  "babel.config.js",
  "metro.config.js",
  "jest.config.js"
],
  "extends": "@react-native/typescript-config/tsconfig.json"
}
